{
    "name": "collie",
    "description": "An asynchronous event-driven network framework written in D. Contains mqtt, http, flatbuffers, protobuf codec.",
    "copyright": "Copyright (C) 2015-2018, Shanghai Putao Technology Co., Ltd",
    "authors": [
        "Huntlabs.cn"
    ],
    "license": "Apache-2.0",
    "dependencies": {
        "kiss": "~>0.3.2"
    },
    "configurations":[
        {
            "name": "default"
        },
        {
            "name": "debug-mode",
            "versions": [
                "CollieDebugMode"
            ],
            "subConfigurations": {
                "kiss" : "debug-mode"
            }
        }
    ],
    "buildTypes": {
        "debug": {
            "buildOptions": [
                "debugMode",
                "debugInfo"
            ]
        },
        "release": {
            "buildOptions": [
                "releaseMode",
                "inline",
                "optimize"
            ]
        }
    }
}
